{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2020",
"skipLibCheck": true,
"noEmit": true,
  "esModuleInterop": true
  },
  "compileOnSave": true,
  "include": [
    "src"
  ]
}
