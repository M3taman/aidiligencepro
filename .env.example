# API Keys for AIDiligencePro
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key - Required for AI analysis
VITE_OPENAI_API_KEY=sk-your_openai_api_key_here

# Alpha Vantage API Key - For financial data
VITE_ALPHA_VANTAGE_API_KEY=your_alphavantage_api_key_here

# SEC API Key - For SEC filing data (optional)
VITE_SEC_API_KEY=your_sec_api_key_here

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id

# Other Optional API Keys
VITE_NEWS_API_KEY=your_news_api_key_here

